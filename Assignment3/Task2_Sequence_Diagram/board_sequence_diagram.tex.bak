			    \begin{call}{B}{removePiece()}{s2}{pawn}
				\end{call}
				
				\begin{messcall}{B}{addPiece(promoted)}{s2}{}
				\end{messcall}
				
				\begin{messcall}{B}{replace(pawn, promoted)}{PP}{}
				\end{messcall}
				
			\end{call}{G}
