			\begin{call}{G}{promotion(x1, y1, x2, y2, String, Color)}{B}{true}	
				\begin{callself}{B}{move(x1, y1, x2, y2, color)}{boolean}
				\end{callself}
				\begin{sdblock}{alt}{[boolean == false]}
				    \begin{messcall}{B}{false}{G}
